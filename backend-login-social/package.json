{
  "name": "backend-blognet",
  "version": "1.0.0",
  "description": "backend of the full stack blognet project",
  "main": "build/src/server.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "node build/src/server.js",
    "predev": "npm run db:reset",
    "prestart": "npm run build",
    "build": "tsc --project tsconfig.json",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx tsc && npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "lint": "eslint ./src --ext .ts"
  },
  "keywords": [
    "backend",
    "express"
  ],
  "author": "elielton",
  "license": "MIT",
  "dependencies": {
    "@sequelize/postgres": "^7.0.0-alpha.41",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.7",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "vercel": "^34.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-trybe-backend": "^2.1.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "globals": "^15.1.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
