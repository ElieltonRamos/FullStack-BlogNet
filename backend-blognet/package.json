{
  "name": "backend-blognet",
  "version": "1.0.0",
  "description": "backend of the full stack blognet project",
  "main": "src/build/server.js",
  "scripts": {
    "test": "test",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "predev": "npm run db:reset",
    "build": "tsc --project tsconfig.json",
    "db:reset": "npx tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "lint": "eslint ./src --ext .ts"
  },
  "keywords": [
    "backend",
    "express"
  ],
  "author": "elielton",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.7",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-trybe-backend": "^2.1.0",
    "eslint-plugin-sonarjs": "^1.0.3",
    "globals": "^15.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
